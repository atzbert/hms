###
# @name List all Ideas
GET {{host}}/restapis/{{api_id}}/local/_user_request_/hackathon/{{hackathon_id}}/ideas

###
# @name Create Idea
POST {{host}}/restapis/{{api_id}}/local/_user_request_/idea
Content-Type: application/json

{
  "ownerId": "{{participant_id}}",
  "hackathonId": "{{hackathon_id}}",
  "title": "Wicked awesome idea",
  "description": "By doing all the things and being the greatest of all time hackathons can be great again",
  "problem": "Hackathons have become weak and boring",
  "goal": "Make hackathons great again",
  "requiredSkills": [
    "{{skill_id}}"
  ],
  "categoryId": "{{category_id}}"
}

###
# @name Get Idea
GET {{host}}/restapis/{{api_id}}/local/_user_request_/idea/{{idea_id}}

###
# @name Remove Idea
DELETE {{host}}/restapis/{{api_id}}/local/_user_request_/idea/{{idea_id}}
